<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="recordCache" class="cn.ihoway.redis.RecordCache"/>
    <bean id="record" class="cn.ihoway.entity.Record"/>
    <bean id="recordSearchProcessor" class="cn.ihoway.processor.RecordSearchProcessor"/>
    <bean id="recordAddProcessor" class="cn.ihoway.processor.RecordAddProcessor"/>
    <bean id="recordServiceImpl" class="cn.ihoway.Impl.RecordServiceImpl"/>


</beans>